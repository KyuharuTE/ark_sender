{
    "manifest_version": 4,
    "type": "extension",
    "name": "ARK Sender",
    "slug": "ark_sender",
    "description": "一个用于发送 ARK 的插件",
    "version": "1.0.0",
    "authors": [{
        "name": "Res91",
        "link": "https://github.com/KyuharuTE"
    }],
    "repository": {
        "repo": "KyuharuTE/ark_sender",
        "branch": "master"
    },
    "dependencies": ["euphony", "NapCat.Framework"],
    "platform": ["win32", "linux", "darwin"],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}
